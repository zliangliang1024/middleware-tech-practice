<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="redis.demo.breakdown.server.mapper.ItemMapper123">

    <!--
        private Integer id;
        private String code;
        private String name;
        private String createTime;
     -->

    <sql id="fields">
        id,code,createTime create_time
    </sql>


    <select id="selectItemByCode1" parameterType="java.lang.String"
            resultType="redis.demo.breakdown.server.entity.Item">
        SELECT
        <include refid="fields"/>
        FROM
        item
        WHERE
        code = #{code}
    </select>

</mapper>